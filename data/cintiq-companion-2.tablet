Width=12
Height=7
Layout=cintiq-companion-2.svg
Styli=@intuos5;@intuos4;
IntegratedIn=Display;System

[Features]
Stylus=true
Touch=true
Ring=false
Buttons=11

[Buttons]
Left=A;B;C;D;E;F;G;H;I;J;K
